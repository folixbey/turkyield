local player = game.Players.LocalPlayer

-- Bildirim Fonksiyonu
local function createNotification()
    local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
    local frame = Instance.new("Frame", screenGui)
    local imageLabel = Instance.new("ImageLabel", frame)
    local titleLabel = Instance.new("TextLabel", frame)
    local textLabel = Instance.new("TextLabel", frame)

    -- Frame Ayarları
    frame.Size = UDim2.new(0, 400, 0, 150)
    frame.Position = UDim2.new(1, -420, 1, -170)
    frame.BackgroundColor3 = Color3.new(0, 0, 0)
    frame.BackgroundTransparency = 0.5

    -- Resim Ayarları
    imageLabel.Size = UDim2.new(0, 100, 0, 100)
    imageLabel.Position = UDim2.new(0, 10, 0, 25)
    imageLabel.Image = "rbxassetid://<YOUR_IMAGE_ASSET_ID>"  -- Buraya resim asset ID'sini ekleyin
    imageLabel.BackgroundTransparency = 1

    -- Başlık Ayarları
    titleLabel.Size = UDim2.new(0, 270, 0, 50)
    titleLabel.Position = UDim2.new(0, 120, 0, 10)
    titleLabel.Text = "Türk Gui"
    titleLabel.TextColor3 = Color3.new(1, 1, 1)
    titleLabel.TextScaled = true
    titleLabel.BackgroundTransparency = 1

    -- Metin Ayarları
    textLabel.Size = UDim2.new(0, 270, 0, 70)
    textLabel.Position = UDim2.new(0, 120, 0, 60)
    textLabel.Text = "En Iyi Script\n\nfolixbey Tarafından Yapılmıştır."
    textLabel.TextColor3 = Color3.new(1, 1, 1)
    textLabel.TextScaled = true
    textLabel.BackgroundTransparency = 1

    screenGui.Parent = player:WaitForChild("PlayerGui")

    wait(5)  -- Bildirimin ne kadar süreyle görüneceğini belirleyin

    screenGui:Destroy()
end

-- Bildirimi 3 kez göster
for i = 1, 3 do
    createNotification()
end

loadstring(game:HttpGet("https://raw.githubusercontent.com/folixbey/turkyield/main/turkiyedl"))()
